{
  "name": "v-ymap",
  "version": "2.0.1",
  "private": false,
  "author": {
    "name": "koeshiro kagami",
    "email": "koeshiro@yandex.ru"
  },
  "description": "Yandex map for vue",
  "keywords": [
    "yandex",
    "yandex-map",
    "map",
    "vue",
    "vuejs",
    "plugin",
    "maps",
    "yandex-maps",
    "vue3"
  ],
  "homepage": "https://github.com/koeshiro/v-ymap",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "npm run codeceptjs --workspace=e2e",
    "test:server": "npm run dev --workspace=vue-e2e-tests"
  },
  "peerDependencies": {
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@yandex/ymaps3-types": "^0.0.7",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.4"
  },
  "workspaces": [
    "./tests/e2e/",
    "./tests/e2e/vue-e2e-tests"
  ]
}
